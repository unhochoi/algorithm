    "n, k = map(int, input().split())\n",
    "\n",
    "count = 0\n",
    "\n",
    "while(n!=1):\n",
    "    \n",
    "    if (n%k==0):\n",
    "        n = n//k\n",
    "    else:\n",
    "        n -= 1\n",
    "    count += 1\n",
    "    \n",
    "print(count)"
   ]
